{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {},
  "rules": {
    "no-param-reassign": "warn",
    "no-unused-vars": "warn",
    "func-names": "off",
    "no-console": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "consistent-return": "off",
    "arrow-body-style": "off"
  }
}
