{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "func-names": "off",
    "no-console": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "consistent-return": "off",
    "arrow-body-style": "off"
  }
}
